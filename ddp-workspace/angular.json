{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ddp-sdk": {
      "root": "projects/ddp-sdk",
      "sourceRoot": "projects/ddp-sdk/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/ddp-sdk/tsconfig.lib.json",
            "project": "projects/ddp-sdk/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ddp-sdk/src/test.ts",
            "tsConfig": "projects/ddp-sdk/tsconfig.spec.json",
            "karmaConfig": "projects/ddp-sdk/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ddp-sdk/tsconfig.lib.json",
              "projects/ddp-sdk/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
