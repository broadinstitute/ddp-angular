    "ddp-angio": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/ddp-angio",
      "sourceRoot": "projects/ddp-angio/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ddp-angio",
            "index": "projects/ddp-angio/src/index.html",
            "main": "projects/ddp-angio/src/main.ts",
            "polyfills": "projects/ddp-angio/src/polyfills.ts",
            "tsConfig": "projects/ddp-angio/tsconfig.app.json",
            "aot": false,
            "assets": [
              "projects/ddp-angio/src/favicon.ico",
              "projects/ddp-angio/src/favicon-alert.ico",
              "projects/ddp-angio/src/assets",
              {
                "glob": "*.pdf",
                "input": "projects/ddp-angio/src/pdf/",
                "output": "/"
              },
              {
                "glob": "pepperConfig.js",
                "input": "projects/ddp-angio/output-config/",
                "output": "assets/config"
              }
            ],
            "styles": [
              "projects/ddp-angio/src/theme.scss",
              "projects/ddp-angio/src/styles.scss"
            ],
            "scripts": [],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/ddp-angio/src/environments/environment.ts",
                  "with": "projects/ddp-angio/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            },
            "es5": {
              "tsConfig": "projects/ddp-angio/tsconfig.es5.json"
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ddp-angio:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ddp-angio:build:production"
            },
            "es5": {
              "browserTarget": "ddp-angio:build:es5"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ddp-angio:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ddp-angio/src/test.ts",
            "polyfills": "projects/ddp-angio/src/polyfills.ts",
            "tsConfig": "projects/ddp-angio/tsconfig.spec.json",
            "karmaConfig": "projects/ddp-angio/karma.conf.js",
            "assets": [
              "projects/ddp-angio/src/favicon.ico",
              "projects/ddp-angio/src/favicon-alert.ico",
              "projects/ddp-angio/src/assets",
              {
                "glob": "*.pdf",
                "input": "projects/ddp-angio/src/pdf/",
                "output": "/"
              }
            ],
            "styles": [
              "projects/ddp-angio/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ddp-angio/tsconfig.app.json",
              "projects/ddp-angio/tsconfig.spec.json",
              "projects/ddp-angio/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/ddp-angio/e2e/protractor.conf.js",
            "devServerTarget": "ddp-angio:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ddp-angio:serve:production"
            }
          }
        }
      }
    },
